@import "stdio.h";

_(typeid T) init_100: {
    $T x: 100;
    return x;
}

# this will error since K is not defined, before it wouldn't error
_(typeid T $K x) change_100: {
    x: 100;
    return x;
}

_() main: {
    int x: init_100($int);
    i8 y: 127;
    change_100($i8 y);

    @c [printf("x: %d, y: %d\n", x, y);];
}
