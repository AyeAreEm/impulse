@import "base/env.imp";

_ main :: () {
    OS platform: OS.get();

    switch (platform) {
        fall (OS.Windows32) {
            println("windows 32bit");
        }
        case (OS.Windows64) {
            println("windows 64bit");
        }
        case (OS.MacOS) {
            println("macos");
        }
        case (OS.Linux) {
            println("linux");
        }
        case {
            println("unknown os");
        }
    }
}
