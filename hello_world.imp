@import "stdio.h"

struct person: {
    string name
    int age
}

_() main: {
    person base: {
        name: ""
        age: 0
    }

    person me: {
        name: "adrian mehta"
        age: 17
    }

    base.name: me.name;
    @c [
        printf("%s\n", base.name.data);
        printf("%s\n", me.name.data);
    ]
}
