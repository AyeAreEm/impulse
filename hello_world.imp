@import "base/raylib/raylib.imp";

_ main :: () {
    rl.InitWindow(800 600 "Raylib - Impulse");

    loop (!rl.WindowShouldClose()) {
        rl.BeginDrawing();
        rl.ClearBackground(rl.BLACK);
    
        rl.Rectangle rec: |100 100 200 200|;
        rl.DrawRectangleRec(rec rl.RED);
        rl.EndDrawing();
    }

    rl.CloseWindow();
    return 0;
}
