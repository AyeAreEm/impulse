@import "base/io.imp";
@import "base/string.imp";

_ main :: () {
    string word: string.from("hello");
    defer string.dealloc(&word);

    print("enter name: ");
    string name: io.input();
    defer string.dealloc(&name);

    string greet: string.format("%s %s" word.data name.data);
    defer string.dealloc(&greet);

    println("%s" greet.data);
}
