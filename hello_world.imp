@import "stdio.h"

struct greet_someone: {
    string greet
    string name
}

_() main: {
    greet_someone dude: {
        greet: ""
        name: ""
    }

    dude.greet: "hello there, "
    dude.name: "john doe"

    @c [
        printf("%s", dude.greet.data);
        printf("%s", dude.name.data);
    ]
}
