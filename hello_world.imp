@import "base/io.imp";
@import "base/dynamic.imp";

_ main() :: {
    dyn[string] words: dyn.new(string);
    dyn.push(string words str.from("hello world"));
    dyn.push(string words str.from("bye globe"));
    for (words) [word] {
        io.println(word);
        mem.dealloc(word.data);
    }

    mem.dealloc(words.data);
}
