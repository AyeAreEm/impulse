@import "stdio.h";
@import "string.h";

^char(typeid T) type: {
    @c [#T;];
}

int(^char str1 ^char str2) str_compare: {
    @c [return strcmp(str1, str2);];
}

bool(typeid T typeid K) type_compare: {
    return str_compare(type(T) type(K));
}

_() main: {
    bool is_same: type_compare($int $int);
    if (is_same) {
        @c [printf("yippe");];
    } else {
        @c [printf("booo");];
    }
}
