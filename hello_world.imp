struct[T] vector: {
    $T x;
    $T y;
}

struct[T] p_vector: {
    ^$T x;
    ^$T y;
}

struct[T] option: {
    $T value;
    bool none;
}

struct[T] dyn_arr: {
    ^$T data;
    usize len;
    usize cap;
}

_() main: {
    vector[int] pos;
    p_vector[int] p_pos;
    option[int] elem;
    dyn_arr[int] arr;
}
