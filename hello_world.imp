enum Dir :: {
    North;
    South;
    West;
    East;
}

_ main :: () {
    Dir dir: Dir.West;

    switch (dir) {
        case (Dir.North) {
            println("vertical");
        }
        fall (Dir.West) {
            println("horziontal");
        }
        case (Dir.East) {
            println("horziontal");
        }
        case (Dir.South) {
            println("vertical");
        }
    }
}
