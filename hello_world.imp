@import "stdio.h";
@import "base/string.imp";
@import "base/dynamic.imp";

_() main: {
    dyn[string] fruits: dyn_new(string);
    dyn_push(string fruits str_from("apple"));
    dyn_push(string fruits str_from("cherry"));
    dyn_push(string fruits str_from("banana"));

    for (fruits) [fruit] {
        for (fruit) [ch] {
            @c [printf("%c", ch);];
        }
        @c [printf("\n");];
    }
}
