@import "base/io.imp";
@import "base/dynamic.imp";

_() main: {
    dyn[string] greets: dyn_new(string);

    dyn_push(string greets str_from("hello world"));
    dyn_push(string greets str_from("hi there"));

    for (greets) [greet] {
        str_println(greet);
        dealloc(greet.data);
    }
    dealloc(greets.data);
}
