@import "base/io.imp";

int main :: () {
    string hello :: string.from("hello");
    string.push_cstr(&hello " world"); # error

    string word :: string.from("hello world");
    string what: string.from("bye world");

    ^string word_ptr: &word; # error
    what: word_ptr^;

    @c [printf("%s\n", what.data);];
}
