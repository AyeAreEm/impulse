@import "stdio.h";

int fib :: (int nth) {
    if (nth = 0 or nth = 1) {return nth;}
    orif (nth = 2) {return 1;}
    else {
        int next: fib([nth-1]);
        int further: fib([nth-2]);
        return [next + further];
    }
}

_ main :: () {
    println("%d" fib(5));

    int a: 1;
    int b: 0;

    loop (i <= 5) [+] {
        int tmp: [a + b];
